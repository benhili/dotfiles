{
  "coc.preferences.formatOnSaveFiletypes": [
    "markdown",
    "javascript",
    "typescript",
    "graphql",
    "html",
    "yaml",
    "json",
    "css",
    "graphql",
    "javascriptreact",
    "typescriptreact"
  ],
  "languageserver": {
    "intelephense": {
      "command": "intelephense",
      "args": ["--stdio"],
      "filetypes": ["php"],
      "initializationOptions": {
        "storagePath": "/tmp/intelephense"
      }
    }
  }
}
